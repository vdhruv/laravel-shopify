{
    "name": "vdhruv/laravel-shopify",
    "description": "Laravel shopify package",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhruv Vadodariya",
            "email": "dhruvvadodariya03@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/support": "^9.34"
    },
    "autoload": {
        "psr-4": {
            "Vdhruv\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vdhruv\\Shopify\\ShopifyServiceProvider"
            ],
            "aliases": {
                "Shopify": "Vdhruv\\Shopify\\Facades\\Shopify"
            }
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
